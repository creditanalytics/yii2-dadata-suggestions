{
    "name": "efureev/yii2-dadata-suggestions",
    "description": "Yii2 component DaData suggestions",
    "type": "yii2-extension",
    "keywords": ["yii2", "suggestions", "dadata"],
    "license": "MIT",
    "version": "0.0.4",
    "authors": [
        {
            "name": "Eugene Fureev",
            "email": "fureev@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "hflabs/suggestions-jquery": "16.8.0",
        "bower-asset/devbridge-autocomplete": "*"
    },
    "repositories":[
        {
            "type":"package",
            "package":{
                "name":"hflabs/suggestions-jquery",
                "version":"16.8.0",
                "source":{
                    "type":"git",
                    "url":"https://github.com/hflabs/suggestions-jquery",
                    "reference":"master"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "efureev\\dadata\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
