{
    "name": "creditanalytics/yii2-dadata-suggestions",
    "description": "Yii2 component DaData suggestions",
    "type": "yii2-extension",
    "keywords": ["yii2", "suggestions", "dadata"],
    "license": "MIT",
    "version": "1.0.5",
    "authors": [
        {
            "name": "Eugene Fureev",
            "email": "fureev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "hflabs/suggestions-jquery": "*",
        "bower-asset/devbridge-autocomplete": "*"
    },
    "repositories":[
        {
          "type":"git",
          "url":"https://github.com/hflabs/suggestions-jquery",
        }
    ],
    "autoload": {
        "psr-4": {
            "creditanalytics\\dadata\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
